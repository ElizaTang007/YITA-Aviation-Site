{
  "version": 2,
  "name": "yita-aviation-site",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/command-center-pro",
      "dest": "/command-center-pro.html"
    },
    {
      "src": "/developer-portal", 
      "dest": "/developer-portal.html"
    },
    {
      "src": "/marketplace",
      "dest": "/marketplace.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html"
    }
  ],
  "headers": [
    {
      "source": "/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}